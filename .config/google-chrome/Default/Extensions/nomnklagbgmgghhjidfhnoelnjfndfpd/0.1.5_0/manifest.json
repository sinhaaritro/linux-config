{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "data/inject.js" ],
      "match_about_blank": true,
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Prevent HTML canvas element from generating a unique identification key to protect user's privacy",
   "homepage_url": "https://add0n.com/canvas-fingerprint-blocker.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "19": "data/icons/19.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "38": "data/icons/38.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Canvas Blocker - Fingerprint Protect",
   "options_ui": {
      "chrome_style": true,
      "page": "data/options/index.html"
   },
   "page_action": {
   },
   "permissions": [ "*://*/*", "notifications", "storage", "webNavigation", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.1.5"
}
